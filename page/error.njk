{% extends "page.njk" %}
{% set mainClasses = "govuk-main-wrapper--l" %}

{% block content %}
  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

        {% for error in error.errors %}
            <h1 class="govuk-heading-xl">{{ error.title }}</h1>
            <p class="govuk-body">
                {{ error.detail }}
            </p>
        {% endfor %}

    </div>
  </div>
{% endblock %}
